                    android:layout_height="wrap_content"
                    android:paddingTop="3dip"
                    android:text="Time :  "
                    android:textColor="#fff" android:textSize="20dp" android:textStyle="bold"/>

                <TextView
                    android:id="@+id/time"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="3dip"
                    android:textColor="#fff" android:textSize="20dp" android:textStyle="bold"/>

            </TableRow>

