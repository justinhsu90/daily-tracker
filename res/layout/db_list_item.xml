                    android:id="@+id/TextView01"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="3dip"
                    android:text="ID :  "
                    android:textColor="#fff" android:textSize="20dp" android:textStyle="bold"/>

                <TextView
                    android:id="@+id/id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="3dip"
                    android:textColor="#fff" android:textSize="20dp" android:textStyle="bold"/>

            </TableRow>

